public class EmployeeTEST {
// 1) 배열을 전달받아서 그 배열에서 가장 큰 값을 반환하는 메소드
// 2) 배열과 숫자를 전달받아서 그 배열에 숫자가 포함되어있는지 여부를 반환하는 메소드

	//속성
	int[] list;
	int yourNumber;


	//기능
	public int mostNumber() {
		int max = 0;
		for(int a=0; a<list.length; a++) {
			if(list[a] > max ) {
				max = list[a];
			}
		}
		return max;
	}

		public boolean yesNumber() {
			boolean yes = false;
			for(int b=0; b<list.length; b++) {
				if(list[b] == yourNumber) {
					yes++;
				} 
			}
			return yes;
		}
}