public class MethodDemo1 {
	
	public static void a() {   // 메소드의 구현
		System.out.println("사용자가 정의한 a 메소드입니다.");	
	}
	
	// 매개변수를 가지고있는 메소드 정의
	// 매개변수는 메소드 실행에 필요한 값을 전달받아서 담는다.
	public static void b(int x) {
		System.out.printf("사용자가 정의한 b 메소드입니다. 값은 %d \n",x);	
	}

	public static void c (String x) {
		System.out.printf("사용자가 정의한 c 메소드입니다." + x + "\n" );	
	}
	
	public static void d (int d, int y) {
		System.out.printf("사용자가 정의한 d 메소드입니다. 값은 %d , %d \n",d,y);
	}

	public static void e (String name, String email, int age) {
		System.out.printf("사용자가 정의한 d 메소드입니다. 값은 : %s %s %d",name,email,age);
	}


	public static void main(String[] args) {
	a();     // 메소드를 호출
	b(100);  // 매개변수가 있는 메소드 호출
	int k = 20;
	b(k);
	
	c("홍길동");
	c("안녕, " + "저는 홍길동 입니다");

	d(1,2);

	e("이름칸" , "test@mail.com" , 99);

	}
}

