public class ArratDemo4 {
	public static void main(String[] args) {
		String[][] groups = new String[2][3];
		
		groups[0][0] = "황진수";
			groups[0][1] = "진희남";
				groups[0][2] = "장해영";
		groups[1][0] = "홍승환";
			groups[1][1] = "윤용학";
				groups[1][2] = "이형준";
		// 다차원 배열 값 표시하기

		for (int i=0; i<2; i++){
			for (int j=0; j<3; j++){
				System.out.println(groups[i][j] + "\t");
			}
			System.out.println( );
		}
		
		

	}
}
